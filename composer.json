{
    "name":        "netcommons/auth-general",
    "description": "General Auth Plugin for CakePHP",
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "auth"],
    "homepage":    "http://www.netcommons.org/",
    "license":     "NetCommons License",
    "authors": [
        {
            "name":     "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/AuthGeneral/graphs/contributors"
        }
    ],
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "netcommons/auth":              "dev-master",
        "netcommons/net-commons":       "dev-master",
        "netcommons/roles":             "dev-master",
        "netcommons/user-roles":        "dev-master"
    },		
    "config": {		
        "vendor-dir": "vendors"	
    }
}
